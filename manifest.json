{
  "manifest_version": 3,
  "name": "Tab Switcher +",
  "description":"Fast switch between tabs and windows. Quick search filter. Export all open URLs with a click.",
  "version": "1.1",
  "icons":{"128": "icon-128.png"},
  "permissions": [
    "tabs",
    "favicon"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icon-128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
